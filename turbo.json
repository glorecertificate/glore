{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "config/**",
    ".env",
    ".node-version",
    "package.json",
    "pnpm-workspace.yaml",
    "tsconfig.json",
    "turbo.json"
  ],
  "globalEnv": [
    "APP_URL",
    "COOKIE_PREFIX",
    "NEXT_TELEMETRY_DISABLED",
    "NODE_ENV",
    "SUPABASE_DB_URL",
    "SUPABASE_TOKEN",
    "TURBO_TEAM",
    "TURBO_TELEMETRY_DISABLED",
    "TURBO_TOKEN",
    "VERCEL_TELEMETRY_DISABLED"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "ENABLE_EXPERIMENTAL_COREPACK",
        "OPENAI_API_KEY",
        "OPENAI_MODEL",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_PASSWORD",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_USER",
        "SUPABASE_ANON_KEY",
        "SUPABASE_JWT_SECRET",
        "SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_URL"
      ],
      "outputs": [".next/**", "!.next/cache/**", ".vercel/output/**", "build/**"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true,
      "env": ["OPENAI_API_KEY", "OPENAI_MODEL", "STORAGE_URL", "SUPABASE_ANON_KEY", "SUPABASE_URL"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["OPENAI_API_KEY", "OPENAI_MODEL", "STORAGE_URL", "SUPABASE_ANON_KEY", "SUPABASE_URL"]
    },
    "watch": {
      "cache": false,
      "persistent": true
    },
    "typegen": {
      "dependsOn": ["^typegen"],
      "outputs": ["**/*.d.ts", "supabase/types.ts"]
    },
    "type-check": {
      "dependsOn": ["^type-check"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "db": {
      "cache": false,
      "dependsOn": ["^db"],
      "outputs": ["supabase/**"]
    }
  }
}
